{
	"compilerOptions": {
		"target": "ES2018",
		"noImplicitAny": true,
		"strictPropertyInitialization": true,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"types": [],
		"baseUrl": "src",
		"paths": {
			"birch": [ "../../birch/src/index" ],
			"elm-app": [ "../../elm-app/src/index" ]
		}
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"src/old/**/*.ts"
	]
}
