{
	"attributeLocations": {
		"a_position": 0,
		"a_uv": 1
	},
	"vertex": [
		"#version 300 es",
		"in vec2 a_position;",
		"in vec2 a_uv;",
		"uniform stage {",
		"  vec2 viewPosition;",
		"  vec2 viewSize;",
		"};",
		"uniform model {",
		"  vec2 position;",
		"  float rotation;",
		"  vec2 scale;",
		"  float level;",
		"};",
		"out vec2 v_uv;",
		"void main() {",
		"  vec2 worldPosition = vec2(position.x + scale.x * a_position.x * cos(rotation) - scale.y * a_position.y * sin(rotation),",
		"                            position.y + scale.y * a_position.y * cos(rotation) + scale.x * a_position.x * sin(rotation));",
		"  vec2 viewPosition = worldPosition - viewPosition;",
		"  gl_Position = vec4(viewPosition / viewSize, 1.0, 1.0);",
		"  v_uv = a_uv;",
		"}"],
	"fragment": [
		"#version 300 es",
		"precision highp float;",
		"uniform sampler2D colorTexture;",
		"in vec2 v_uv;",
		"out vec4 o_color;",
		"void main() {",
		"  o_color = texture(colorTexture, v_uv);",
		"}"],
	"blending": "mix"
}